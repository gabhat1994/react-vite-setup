  "scripts": {
    "dev": "yarn build:pwa && yarn build:firebase development && vite",
    "build": "tsc && yarn build:assets && yarn build:pwa && yarn build:firebase development && yarn build:vite --mode development",
    "build:devprod": "tsc && yarn build:assets && yarn build:pwa && yarn build:firebase development && yarn build:vite --mode development",
    "build:prod": "tsc & yarn build:assets && yarn build:pwa && yarn build:firebase prod && yarn build:vite --mode production",
    "build:uat": "tsc && yarn build:assets && yarn build:pwa && yarn build:firebase uat && yarn build:vite --mode uat",
    "build:uat2": "tsc && yarn build:assets && yarn build:pwa && yarn build:firebase uat2 && yarn build:vite --mode uat",
    "build:staging": "tsc & yarn build:assets && yarn build:pwa && yarn build:firebase staging && yarn build:vite --mode staging",
    "build:nightly": "tsc & yarn build:assets && yarn build:pwa && yarn build:firebase nightly && yarn build:vite --mode nightly",
    "build:assets": "yarn build:styles && yarn build:reactpdf",
    "build:firebase": "node build/firebase/build.js",
    "build:pwa": "node build/pwa/build.js",
    "build:styles": "node build/style-dictionary/build.js",
    "build:reactpdf": "node build/react-pdf/build.js",
    "build:vite": "export NODE_OPTIONS=--max_old_space_size=8192 || set NODE_OPTIONS=--max_old_space_size=8192 && vite build",
    "build:glass-icons": "node build/glassIcons/build.js",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx  --fix src",
    "format": "npx pretty-quick --staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "NODE_OPTIONS=--max_old_space_size=8192 storybook build",
    "test": "vitest",
    "test:all": "vitest --run --coverage && tsc && yarn lint",
    "test-with-coverage": "vitest --run --coverage",
    "test-without-coverage": "vitest --run",
    "rebuild": "export NODE_OPTIONS=--max_old_space_size=8192 || set NODE_OPTIONS=--max_old_space_size=8192&&export NODE_ENV=development || set NODE_ENV=development && yarn install && vite build --mode development",
    "cy": "yarn cypress:open:point-to-local:build",
    "cypress:open:point-to-local": "export NODE_ENV=development || set NODE_ENV=development&&concurrently \"yarn dev\" \"yarn cypress open --config-file cypress.config.development.ts\"",
    "cypress:open:point-to-local:build": "export NODE_ENV=development || set NODE_ENV=development&& concurrently \"vite preview --port 3000\" \"yarn cypress open  --config-file cypress.config.development.ts\"",
    "cypress:run:point-to-local": "export NODE_ENV=development || set NODE_ENV=development&&concurrently \"yarn dev\" \"yarn cypress run --config-file cypress.config.development.ts\"",
    "cypress:run:point-to-nightly": "export NODE_ENV=nightly || set NODE_ENV=nightly&&yarn cypress run --config-file cypress.config.nightly.ts",
    "cypress:open:point-to-nightly": "export NODE_ENV=nightly || set NODE_ENV=nightly&&yarn cypress open --config-file cypress.config.nightly.ts",
    "cypress:run:point-to-staging": "export NODE_ENV=staging || set NODE_ENV=staging&&yarn cypress run --config-file cypress.config.staging.ts --browser chrome",
    "cypress:open:point-to-staging": "export NODE_ENV=staging || set NODE_ENV=staging&&yarn cypress open --config-file cypress.config.staging.ts",
    "cypress:run:point-to-staging-sanity": "export NODE_ENV=staging || set NODE_ENV=staging&&yarn cypress run --config-file cypress.config.staging.sanity.ts --browser chrome",
    "cypress:run:point-to-uat-sanity": "export NODE_ENV=uat || set NODE_ENV=uat&&yarn cypress run --config-file cypress.config.uat.sanity.ts",
    "cypress:open:point-to-uat-sanity": "export NODE_ENV=uat || set NODE_ENV=uat&&yarn cypress open --config-file cypress.config.uat.sanity.ts",
    "cypress:run:point-to-uat": "export NODE_ENV=uat || set NODE_ENV=uat&&yarn cypress run --config-file cypress.config.uat.ts --browser chrome",
    "cypress:open:point-to-uat": "export NODE_ENV=uat || set NODE_ENV=uat&&yarn cypress open --config-file cypress.config.uat.ts",
    "cypress:run:point-to-prod-sanity": "export NODE_ENV=production || set NODE_ENV=production&&yarn cypress run --config-file cypress.config.prod.sanity.ts --browser chrome",
    "cypress:open:point-to-prod-sanity": "export NODE_ENV=production || set NODE_ENV=production&&yarn cypress open --config-file cypress.config.prod.sanity.ts --browser chrome",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "generate": "graphql-codegen --config .graphqlrc",
    "sort": "sort-json src/translations/en-us.json -i",
    "basic-checks": "yarn run tsc --skiplibcheck && yarn lint && yarn sort && yarn format",
    "lint:unused-exports": "npx ts-prune -i '.stories.tsx|src/apollo'"
  },
